{
    "name": "公租房抢房插件",
    "description": "公租房抢房插件",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": [
        "http://*/*",
        "https://*/*",
        "background",
        "notifications",
        "management"
    ],
    "icons": {
        "48": "images/icon-48.png"
    },
    "background": {
        "scripts": ["jquery-1.9.1.js", "background.js"]
    },
    "browser_action": {
        "default_title": "公租房抢房插件",
        "default_icon": "images/icon-48.png",
        "default_popup": "popup.html"
    },
    "options_page": "setting.html"
}
